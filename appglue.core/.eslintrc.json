{
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "comma-dangle": "off",
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1,
                "CallExpression": {
                    "arguments": "first"
                }
            }
        ],
        "react/jsx-filename-extension": "off"
    }
}
